{
  "manifest_version": 2,
  
  "name": "Chrome Extension for REST API",
  "description": "Fetches the list of OS and browsers and submits a test.",
  "version": "1.0",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Test your URL"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}